[*.{cs,vb}]

# IDE0060: Remove unused parameter
dotnet_code_quality_unused_parameters = non_public:silent

# CS1998: Async method lacks 'await' operators and will run synchronously
dotnet_diagnostic.CS1998.severity = suggestion

# CS0649: Field 'DiscordApiKey.jPath' is never assigned to, and will always have its default value null
dotnet_diagnostic.CS0649.severity = none

# IDE0055: Fix formatting
dotnet_diagnostic.IDE0055.severity = suggestion

# CS0162: Unreachable code detected
dotnet_diagnostic.CS0162.severity = suggestion
